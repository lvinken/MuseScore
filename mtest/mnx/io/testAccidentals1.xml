<?xml version="1.0" encoding="UTF-8"?>
<mnx>
    <head>
        <title>Accidentals 1</title>
        <subtitle>MuseScore testfile</subtitle>
        <creator type="composer">Leon Vinken</creator>
    </head>
    <score>
        <mnx-common profile="standard">
            <global>
                <measure>
                    <directions>
                        <time signature="3/4"/>
                    </directions>
                </measure>
                <measure/>
            </global>
            <part>
                <part-name>Music</part-name>
                <measure>
                    <directions>
                        <clef line="2" sign="G"/>
                    </directions>
                    <sequence>
                        <event value="/4">
                            <note pitch="G4"/>
                        </event>
                        <event value="/4">
                            <note accidental="sharp" pitch="A#4"/>
                        </event>
                        <event value="/4">
                            <note accidental="flat" pitch="Bb4"/>
                        </event>
                    </sequence>
                </measure>
                <measure>
                    <sequence>
                        <event value="/4">
                            <note accidental="sharp-sharp" pitch="C##5"/>
                        </event>
                        <event value="/4">
                            <note accidental="flat-flat" pitch="Dbb5"/>
                        </event>
                        <event value="/4">
                            <note pitch="D5"/>
                        </event>
                    </sequence>
                </measure>
            </part>
        </mnx-common>
    </score>
</mnx>
