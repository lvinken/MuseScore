<?xml version="1.0" encoding="UTF-8"?>
<mnx>
    <head>
        <title>Complete measure rest</title>
        <subtitle>MuseScore testfile</subtitle>
        <creator type="composer">Leon Vinken</creator>
    </head>
    <score>
        <mnx-common profile="standard">
            <global>
                <measure>
                    <directions>
                        <time signature="3/4"/>
                    </directions>
                </measure>
                <measure/>
            </global>
            <part>
                <part-name>Music</part-name>
                <measure>
                    <directions>
                        <clef line="2" sign="G"/>
                    </directions>
                    <sequence>
                        <event value="/4">
                            <rest/>
                        </event>
                        <event value="/4">
                            <rest/>
                        </event>
                        <event value="/4">
                            <rest/>
                        </event>
                    </sequence>
                </measure>
                <measure>
                    <sequence>
                        <event measure="yes">
                            <rest/>
                        </event>
                    </sequence>
                </measure>
            </part>
        </mnx-common>
    </score>
</mnx>
