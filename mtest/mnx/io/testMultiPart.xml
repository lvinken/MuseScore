<?xml version="1.0" encoding="UTF-8"?>
<mnx>
    <head>
        <title>Multi part</title>
        <subtitle>MuseScore testfile</subtitle>
        <creator type="composer">Leon Vinken</creator>
    </head>
    <score>
        <cwmnx profile="standard">
            <global>
                <measure>
                    <directions>
                        <time signature="4/4"/>
                    </directions>
                </measure>
            </global>
            <part>
                <part-name>Violin</part-name>
                <measure>
                    <directions>
                        <clef line="2" sign="G"/>
                    </directions>
                    <sequence>
                        <event value="/1">
                            <note pitch="G4"/>
                        </event>
                    </sequence>
                </measure>
            </part>
            <part>
                <part-name>Cello</part-name>
                <measure>
                    <directions>
                        <clef line="4" sign="F"/>
                    </directions>
                    <sequence>
                        <event value="/1">
                            <note pitch="C2"/>
                        </event>
                    </sequence>
                </measure>
            </part>
        </cwmnx>
    </score>
</mnx>
