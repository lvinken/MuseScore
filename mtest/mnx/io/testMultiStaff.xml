<?xml version="1.0" encoding="UTF-8"?>
<mnx>
    <head>
        <title>Multi staff</title>
        <subtitle>MuseScore testfile</subtitle>
        <creator type="composer">Leon Vinken</creator>
    </head>
    <score>
        <cwmnx profile="standard">
            <global>
                <measure>
                    <directions>
                        <time signature="4/4"/>
                    </directions>
                </measure>
            </global>
            <part>
                <part-name>Music</part-name>
                <measure>
                    <directions>
                        <staves number="2"/>
                        <clef line="2" sign="G" staff="1"/>
                        <clef line="4" sign="F" staff="2"/>
                    </directions>
                    <sequence staff="1">
                        <event value="/1">
                            <note pitch="G4"/>
                        </event>
                    </sequence>
                    <sequence staff="2">
                        <event value="/1">
                            <note pitch="G3"/>
                        </event>
                    </sequence>
                </measure>
            </part>
        </cwmnx>
    </score>
</mnx>
